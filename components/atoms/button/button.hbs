{{#if typeButton}}
	<button {{#id}}id="{{id}}"{{/id}} class="button">{{title}}</button>
{{else}}
	{{#if typeInput}}
		<input {{#id}}id="{{id}}"{{/id}} type="submit" class="button" value="{{title}}" />
	{{else}}
		<a href="{{#url}}{{url}}{{/url}}{{^url}}javascript:void(0);{{/url}}" {{#id}}id="{{id}}"{{/id}} class="button">{{title}}</a>
	{{/if}}
{{/if}}
